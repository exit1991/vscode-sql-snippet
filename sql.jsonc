	}
	
	
	//その他キーワード
	,"単純CASE式": {
		"prefix": "CASE .. WHEN ... END",
		"body": [
			"CASE ${1:カラム名}",
			"WHEN ${2:値1} THEN ${3:処理1}",
			"WHEN ${4:値2} THEN ${5:処理2}",
			"ELSE ${6:その他の処理} END",
			""
		],
		"description": "CASE"
	}
	
	,"検索CASE式": {
		"prefix": "CASE WHEN ... END",
		"body": [
			"CASE WHEN ${1:条件1} THEN ${2:処理1}",
			"     WHEN ${3:条件2} THEN ${4:処理2}",
			"     ELSE ${5:その他の処理} END",
			""
		],
		"description": "CASE"
	}
	
}