	// 演算子
	,"BETWEEN演算子": {
		"prefix": "BETWEEN .. AND ..",
		"body": ["BETWEEN ${1:最小値} AND ${2:最大値}"],
		"description": "BETWEEN"
	}
	
	,"IN演算子": {
		"prefix": "IN(.., ..)",
		"body": ["IN(${1:値}, ${2:値})"],
		"description": "IN"
	}
	
	,"LIKE演算子": {
		"prefix": "LIKE '..'",
		"body": ["LIKE '${1:文字列 % _ など}'"],
		"description": "LIKE"
	}
	
	
	// 条件
	,"AND": {
		"prefix": "AND",
		"body": ["AND $1"],
		"description": "AND"
	}
	
	,"OR": {
		"prefix": "OR",
		"body": ["OR $1"],
		"description": "OR"
	}
	
	,"NOT": {
		"prefix": "NOT",
		"body": ["NOT $1"],
		"description": "NOT"
	}
	
	
	// グループ関数
	,"COUNT関数": {
		"prefix": "COUNT(..)",
		"body": ["COUNT(${1:カラム名／値})"],
		"description": "COUNT"
	}
	
	,"SUM関数": {
		"prefix": "SUM(..)",
		"body": ["SUM(${1:カラム名／値})"],
		"description": "SUM"
	}
	
	,"MIN関数": {
		"prefix": "MIN(..)",
