{
	// Place your snippets for sql here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	
	"SELECT文 短め 句内改行なし": {
		"prefix": "SELECT FROM WHERE",
		"body": [
			"SELECT ${1:カラム名}",
			"FROM ${2:テーブル名}",
			"WHERE ${3:条件}",
			";"
		],
		"description": "SELECT"
	}
	
	,"SELECT文 短め 句内改行あり": {
		"prefix": "SELECT FROM WHERE",
		"body": [
			"SELECT",
			"    ${1:カラム名}",
			"FROM",
			"    ${2:テーブル名}",
			"WHERE",
			"    ${3:条件}",
			";"
		],
		"description": "SELECT"
	}
	
	,"SELECT文 長め 句内改行なし": {
		"prefix": "SELECT FROM WHERE .. ORDER BY",
		"body": [
			"SELECT ${1:カラム名}",
			"FROM ${2:テーブル名}",
			"WHERE ${3:条件}",
			"GROUP BY ${4:カラム名}",
			"HAVING ${5:条件}",
			"ORDER BY ${6:カラム名 DESC}",
			";"
		],
		"description": "SELECT"
	}
	
	,"SELECT文 長め 句内改行あり": {
		"prefix": "SELECT FROM WHERE .. ORDER BY",
		"body": [
			"SELECT",
			"    ${1:カラム名}",
			"FROM",
			"    ${2:テーブル名}",
			"WHERE",
			"    ${3:条件}",
			"GROUP BY",
			"    ${4:カラム名}",
			"HAVING",
			"    ${5:条件}",
			"ORDER BY",
			"    ${6:カラム名 DESC}",
			";"
		],
		"description": "SELECT"
	}
	
	,"SELECT COUNT": {
		"prefix": "SELECT COUNT(*)",
		"body": [
			"SELECT COUNT(*)",
			"FROM ${1:テーブル名}",
			";"
		],
		"description": "SELECT"
	}
	
	
	
